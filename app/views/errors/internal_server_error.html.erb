<div class="errors-wrapper theme">
  <div class="error-div">
    <div class="error-msg-div">
      <div class="error-msg">
        <ul class="error-list">
          <li class="error-list-first">
            <h1>
            Error <button class="error-btn">500</button>
            </h1>
          </li>
          <li class="error-list-second">
            <h1 style="margin-top: 0px !important;">
              Sorry, we have encountered an internal server error.
            </h1>
          </li>
          <li class="error-list-third">
            <h1 style="margin-top: 50px !important; margin-bottom: 20px !important;">
              <button class="navbar-wagon-button return-home-button">
                <%= link_to 'BACK HOME', root_path, class: 'return-home-link' %>
              </button>
            </h1>
          </li>
        </ul>
      </div>
    </div>
  </div>
</div>
